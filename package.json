{
  "name": "siyuan-plugin-ai-graph",
  "version": "0.0.1",
  "description": "A SiYuan note plugin that generates an inverted index based on semantic segmentation of non-structured articles in SiYuan notes, and visualizes the relationships between articles using a knowledge graph.",
  "type": "module",
  "repository": "terwer/siyuan-plugin-ai-graph",
  "homepage": "https://github.com/terwer/siyuan-plugin-ai-graph",
  "author": "terwer",
  "license": "MIT",
  "scripts": {
    "makeLink": "python scripts/make_dev_link.py",
    "serve": "vite",
    "dev": "vite build --watch",
    "build": "vite build",
    "start": "vite preview",
    "test": "vitest --watch",
    "syncVersion": "python scripts/version.py",
    "parseChangelog": "python scripts/parse_changelog.py",
    "prepareRelease": "pnpm syncVersion && pnpm parseChangelog",
    "package": "python scripts/package.py"
  },
  "devDependencies": {
    "@terwer/eslint-config-custom": "^1.3.4",
    "@types/minimist": "1.2.5",
    "@types/node": "^22.19.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "fast-glob": "^3.3.3",
    "jsdom": "^27.1.0",
    "minimist": "^1.2.8",
    "rollup-plugin-livereload": "^2.0.5",
    "siyuan": "^1.1.5",
    "stylus": "^0.64.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vite-plugin-static-copy": "^3.1.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "vue": "^3.5.13",
    "zhi-lib-base": "^0.8.0"
  },
  "packageManager": "pnpm@10.18.3"
}
